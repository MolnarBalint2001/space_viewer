{
    "name": "teto-b2c-api",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "swc src -d dist --minify",
        "dev": "nodemon src/index.ts -e ts,json,yaml --ignore src/_generated --ignore node-modules --ignore package.json --ignore package-lock.json --ignore tests/ --ignore tmp/",
        "start": "node dist/main.js",
        "typeorm": "npx tsx ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/db/dataSource.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./src/db/dataSource.ts migration:generate ./migrations/$npm_config_name",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
        "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
        "openapi:generate": "openapi-generator-cli generate -i docs/openapi.yaml -g typescript-axios -o ../customer-client/src/config/api",
        "openapi:generate-admin": "openapi-generator-cli generate -i docs/openapi.yaml -g typescript-axios -o ../admin-client/src/config/api",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@google-analytics/data": "^5.1.0",
        "@types/pdf-parse": "^1.1.5",
        "@qdrant/js-client-rest": "^1.15.1",
        "amqplib": "^0.10.9",
        "axios": "^1.12.2",
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^8.0.1",
        "form-data": "^4.0.4",
        "google-auth-library": "^9.14.1",
        "helmet": "^7.1.0",
        "http-errors": "^2.0.0",
        "jsonwebtoken": "^9.0.2",
        "minio": "^8.0.1",
        "morgan": "^1.10.1",
        "multer": "^1.4.5-lts.1",
        "neo4j-driver": "^6.0.0",
        "nodemailer": "^7.0.5",
        "openai": "^6.1.0",
        "pdf-parse": "^2.1.7",
        "pg": "^8.12.0",
        "pino": "^9.3.2",
        "pino-http": "^9.0.0",
        "proj4": "^2.9.1",
        "rate-limiter-flexible": "^5.0.5",
        "redis": "^4.7.1",
        "reflect-metadata": "^0.2.2",
        "shpjs": "^4.0.4",
        "typeorm": "^0.3.20",
        "uuid": "^9.0.1",
        "winston": "^3.17.0",
        "ws": "^8.18.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.22.0",
        "@types/amqplib": "^0.10.7",
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/geojson": "^7946.0.14",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.14.12",
        "@types/nodemailer": "^7.0.0",
        "@types/webpack-node-externals": "^3.0.4",
        "@types/ws": "^8.5.12",
        "copy-webpack-plugin": "^13.0.1",
        "cross-env": "^7.0.3",
        "dotenv-webpack": "^8.1.1",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "swc": "^1.0.11",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.3",
        "typeorm-ts-node-esm": "^0.3.20",
        "typescript": "^5.5.4",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4",
        "webpack-node-externals": "^3.0.0"
    }
}
